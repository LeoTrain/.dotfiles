{
    "to_clean_path": "~/lab/my_projects",
    "log_path": "CleanFolder/activity_log.txt",
    "extensions": {
        "images": [ ".jpg", ".jpeg", ".png", ".gif", ".bmp", ".tiff",
                    ".tif", ".webp", ".heif", ".heic", ".svg", ".raw",
                    ".ico", ".psd", ".ai", ".eps", ".pdf", ".exr", ".indd"],
        "archives":[ ".zip", ".rar", ".tar", ".gz", ".7z", ".bz2", ".xz", 
                     ".tar.gz", ".tar.bz2", ".tar.xz"]
    },
    "organize_subfolders": {
        "images": "Images",
        "archives": "Archives"
    },
    "by_date": true,
    "only_directories": true,
    "recursive": true,
    "depth": 1
}
